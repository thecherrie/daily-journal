<!DOCTYPE html>
<html lang="en" dir="ltr">
  <%- include("partials/header.ejs") -%>

    <% post.forEach(function(post) { %>
        <h2>
          <%= post.postTitle %>
        </h2>
        <p>
          <%= post.postMessage.substring(0, 256); %>
          </p>
          <a href="/posts/<%= post.postTitle %>">Read More...</a>
    <% }); %>

    <h1>Home</h1>
    <p>
       <%= homeStartingContent %>
    </p>
  <%- include("partials/footer.ejs") -%>
</html>
